alias rl="source ~/bin/dotfiles/bashrc" #reload bashrc
alias gs="git status"
alias gh="git help" 
alias gca="git commit -a"
alias gpo="git push -u origin" 
alias tu="top -o cpu" #cpu
alias tm="top -o vsize" #memory
alias html5="rake newhtml5"
alias gph="git push heroku master"
alias project:new="rake project:newproject"
function copy {
	cat $1 | pbcopy
}